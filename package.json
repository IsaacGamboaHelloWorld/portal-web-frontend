{
    "name": "bpop-pb-frontend",
    "version": "3.1.47",
    "config": {
      "port": "4200"
    },
    "localhost": "http://localhost:4200",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "start:local": "cross-var ng serve --proxy-config=proxies/dev-proxy.conf.json --port=$npm_package_config_port",
        "start:local-stg": "cross-var ng serve --proxy-config=proxies/stg-proxy.conf.json --port=$npm_package_config_port",
        "start:local-prod": "cross-var ng serve --proxy-config=proxies/prod-proxy.conf.json --port=$npm_package_config_port",
        "build:prod": "ng build --configuration=pro",
        "build:dev": "ng build --configuration=dev",
        "build:stg": "ng build --configuration=stg",
        "compodoc": "npx compodoc -p src/tsconfig.app.json",
        "precommit": "npm run lint",
        "analyzer": "ng build --configuration=dev --stats-json && webpack-bundle-analyzer dist/stats-es2015.json",
        "coverage": "ng test --watch=false --code-coverage && http-server coverage/ -o",
        "mock:server": "json-server --watch test-helpers/db/db.js",
        "prettier:list": "prettier --list-different \"./{e2e,src}/**/*.{scss,ts}\" || ECHO .",
        "prettier:write": "prettier --write src/app/**/*.ts",
        "prettier:write:sass": "prettier-stylelint --write 'src/**/*.sass'",
        "prettier": "prettier --check src/app/**/*.ts",
        "tslint:check": "tslint-config-prettier-check ./tslint.json",
        "tslint:fix": "tslint --config ./tslint.json --fix --project .",
        "lint:html": "htmlhint src/app/**/*.html",
        "lint:sass": "stylelint src/app/**/**/*.sass --syntax sass",
        "lint:all": "npm run lint && npm run lint:html && npm run lint:sass",
        "build:cache": "node build-cache.config.js",
        "customScript": "node build-cache-sw.config.js",
        "sonar": "sonar-scanner"
    },
    "private": true,
    "dependencies": {
        "@angular-devkit/build-angular": "~0.803.19",
        "@angular/animations": "^8.2.4",
        "@angular/cdk": "^8.1.4",
        "@angular/common": "~8.2.4",
        "@angular/compiler": "~8.2.4",
        "@angular/core": "~8.2.4",
        "@angular/forms": "~8.2.4",
        "@angular/platform-browser": "~8.2.4",
        "@angular/platform-browser-dynamic": "~8.2.4",
        "@angular/router": "~8.2.4",
        "@avaldigitallabs/adl-pfm-lib-web-components": "^2.1.7",
        "@datadog/browser-rum": "^1.24.0",
        "@ng-select/ng-select": "^3.7.3",
        "@ngrx/effects": "^8.3.0",
        "@ngrx/store": "^8.3.0",
        "@ngrx/store-devtools": "^8.3.0",
        "@ngx-translate/core": "^11.0.1",
        "@ngx-translate/http-loader": "^4.0.0",
        "body-scroll-lock": "^2.6.4",
        "cleave.js": "^1.6.0",
        "core-js": "^2.5.4",
        "device-detector-js": "^3.0.2",
        "html-to-image": "^1.6.1",
        "js-sha512": "^0.8.0",
        "lottie-web": "^5.6.4",
        "ng-otp-input": "1.8.1",
        "ng-recaptcha": "^5.0.0",
        "primeng": "^8.0.0",
        "rxjs": "~6.5.2",
        "smooth-scroll": "^16.1.0",
        "tiny-slider": "2.9.1",
        "tslib": "^1.10.0",
        "utf8": "3.0.0",
        "uuid": "^8.3.1",
        "zone.js": "~0.9.1"
    },
    "devDependencies": {
        "@angular/cli": "^8.3.2",
        "@angular/compiler-cli": "~8.2.4",
        "@angular/language-service": "~8.2.4",
        "@compodoc/compodoc": "^1.1.19",
        "@testing-library/dom": "^5.6.1",
        "@types/faker": "^4.1.4",
        "@types/jasmine": "~2.8.8",
        "@types/jasminewd2": "~2.0.3",
        "@types/node": "~8.9.4",
        "cheerio": "^1.0.0-rc.3",
        "codelyzer": "^5.0.1",
        "cross-var": "^1.1.0",
        "faker": "^4.1.0",
        "htmlhint": "^0.11.0",
        "husky": "^2.3.0",
        "jasmine-core": "^3.4.0",
        "jasmine-spec-reporter": "~4.2.1",
        "json-server": "^0.15.1",
        "karma": "^4.1.0",
        "karma-chrome-launcher": "~2.2.0",
        "karma-coverage-istanbul-reporter": "^2.0.5",
        "karma-jasmine": "^2.0.1",
        "karma-jasmine-html-reporter": "^1.4.2",
        "karma-mocha-reporter": "^2.2.5",
        "npm-audit-ci-wrapper": "^2.2.1",
        "npm-run-all": "^4.1.5",
        "prettier": "^1.17.1",
        "prettier-stylelint": "^0.4.2",
        "protractor": "^5.4.2",
        "sonar-scanner": "^3.1.0",
        "stylelint": "^10.1.0",
        "stylelint-config-recommended": "^2.2.0",
        "stylelint-config-sass-guidelines": "^6.0.0",
        "stylelint-config-standard": "^18.3.0",
        "ts-node": "~7.0.0",
        "tslint": "~5.17.0",
        "tslint-etc": "^1.13.10",
        "tslint-clean-code": "^0.2.9",
        "tslint-config-prettier": "^1.18.0",
        "tslint-plugin-prettier": "^2.0.1",
        "typescript": "~3.5.3",
        "webpack": "^4.41.2",
        "webpack-bundle-analyzer": "^3.3.2"
    },
    "husky": {
        "hooks": {
            "pre-commit": "run-s lint lint:html lint:sass prettier"
        }
    }
}
